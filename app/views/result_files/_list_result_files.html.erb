<h3>debug: list of hashes - each one is a file</h3>
  <table class="form-with-columns" border="0">
    <tr>
      <th>Filename</th>
      <th>File Size</th>
      <th>Content Type</th>
      <th>Lab ID</th>
      <th>Updated By</th>
    </tr>
    <% file_list.each do |file_hash| %>
    <tr>
      <td><%= file_hash[:document] %></td>
      <td><%= file_hash[:document_file_size] %></td>
      <td><%= file_hash[:document_content_type] %></td>
      <td><%= file_hash[:lab_id] %></td>
      <td><%= file_hash[:updated_by] %></td>
   </tr>
   <% end %>
</table>



