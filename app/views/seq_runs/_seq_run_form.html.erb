<fieldset class="form">
  <legend>New Sequencing Run</legend>
  <table border="0" class="form">  		  
	<tr>
	  <th>Date:</th>
	  <td><%= f.calendar_date_select(:date_sequenced, :valid_date_check => "date <= (new Date()).stripTime()", :size => 15) %></td>
	</tr>
	<tr>
	  <th>Sequencing Run#</th>
	  <td><%= f.text_field(:seq_run_nr) %></td>
	</tr>
  </table>
</fieldset>
